<% content_for :page_title do %>
<%= render "title" %>
<% end %>

<div class="row">
  <div class="col-md-12">
    <div class="portlet box">
      <div class="dataTables_wrapper form-inline" role="grid">
        <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover" id="trailer_table">
            <thead>
              <tr>
                <th>账号</th>
                <th>角色</th>
                <th>服务商</th>
                <th>客户经理</th>
                <th>客户</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <% @users.each do |f| %>
                <tr>
                  <td><%= f.username %></td>
                  <td><%= f.role %></td>
                  <td></td>
                  <td></td>
                  <td></td>
                  <td>
                    
                  </td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
